{
  "dashboard": {
    "id": null,
    "uid": "lambda-performance",
    "title": "Lambda Performance",
    "tags": ["lambda", "performance", "application"],
    "style": "dark",
    "timezone": "UTC",
    "refresh": "1m",
    "folderId": null,
    "folderUid": "application-folder",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "stage",
          "type": "custom",
          "options": [
            { "text": "dev", "value": "dev", "selected": true },
            { "text": "staging", "value": "staging", "selected": false },
            { "text": "prod", "value": "prod", "selected": false }
          ],
          "current": { "text": "dev", "value": "dev" }
        },
        {
          "name": "function",
          "type": "query",
          "datasource": "CloudWatch-UserMetrics",
          "query": "dimension_values(us-east-1,AWS/Lambda,Invocations,FunctionName)",
          "regex": "/lego-api-serverless-$stage-.*/",
          "multi": true,
          "includeAll": true,
          "current": { "text": "All", "value": "$__all" }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Lambda Invocations (Sum by Function)",
        "type": "stat",
        "targets": [
          {
            "datasource": "CloudWatch-UserMetrics",
            "namespace": "AWS/Lambda",
            "metricName": "Invocations",
            "dimensions": {
              "FunctionName": "$function"
            },
            "statistics": ["Sum"],
            "period": "300"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 100 },
                { "color": "red", "value": 1000 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Lambda Duration (p50, p95, p99)",
        "type": "timeseries",
        "targets": [
          {
            "datasource": "CloudWatch-UserMetrics",
            "namespace": "AWS/Lambda",
            "metricName": "Duration",
            "dimensions": {
              "FunctionName": "$function"
            },
            "statistics": ["p50", "p95", "p99"],
            "period": "300"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "unit": "ms",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 1000 },
                { "color": "red", "value": 5000 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 9, "x": 6, "y": 0 }
      },
      {
        "id": 3,
        "title": "Lambda Errors (Count and Rate)",
        "type": "stat",
        "targets": [
          {
            "datasource": "CloudWatch-UserMetrics",
            "namespace": "AWS/Lambda",
            "metricName": "Errors",
            "dimensions": {
              "FunctionName": "$function"
            },
            "statistics": ["Sum"],
            "period": "300"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 10 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 4, "x": 15, "y": 0 }
      },
      {
        "id": 4,
        "title": "Lambda Throttles",
        "type": "stat",
        "targets": [
          {
            "datasource": "CloudWatch-UserMetrics",
            "namespace": "AWS/Lambda",
            "metricName": "Throttles",
            "dimensions": {
              "FunctionName": "$function"
            },
            "statistics": ["Sum"],
            "period": "300"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "red", "value": 1 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 5, "x": 19, "y": 0 }
      },
      {
        "id": 5,
        "title": "Lambda Cold Starts",
        "type": "stat",
        "targets": [
          {
            "datasource": "CloudWatch-UserMetrics",
            "namespace": "AWS/Lambda",
            "metricName": "InitDuration",
            "dimensions": {
              "FunctionName": "$function"
            },
            "statistics": ["Sum"],
            "period": "300"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "unit": "ms",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 1000 },
                { "color": "red", "value": 3000 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 6, "x": 0, "y": 6 }
      },
      {
        "id": 6,
        "title": "Lambda Concurrent Executions",
        "type": "timeseries",
        "targets": [
          {
            "datasource": "CloudWatch-UserMetrics",
            "namespace": "AWS/Lambda",
            "metricName": "ConcurrentExecutions",
            "dimensions": {
              "FunctionName": "$function"
            },
            "statistics": ["Maximum"],
            "period": "300"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 50 },
                { "color": "red", "value": 100 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 9, "x": 6, "y": 6 }
      },
      {
        "id": 7,
        "title": "Error Rate (%)",
        "type": "stat",
        "targets": [
          {
            "datasource": "CloudWatch-UserMetrics",
            "expression": "(m2/m1)*100",
            "hide": false,
            "refId": "A"
          },
          {
            "datasource": "CloudWatch-UserMetrics",
            "namespace": "AWS/Lambda",
            "metricName": "Invocations",
            "dimensions": {
              "FunctionName": "$function"
            },
            "statistics": ["Sum"],
            "period": "300",
            "refId": "m1"
          },
          {
            "datasource": "CloudWatch-UserMetrics",
            "namespace": "AWS/Lambda",
            "metricName": "Errors",
            "dimensions": {
              "FunctionName": "$function"
            },
            "statistics": ["Sum"],
            "period": "300",
            "refId": "m2"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "unit": "percent",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 5 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 9, "x": 15, "y": 6 }
      },
      {
        "id": 8,
        "title": "Lambda Invocations Over Time",
        "type": "timeseries",
        "targets": [
          {
            "datasource": "CloudWatch-UserMetrics",
            "namespace": "AWS/Lambda",
            "metricName": "Invocations",
            "dimensions": {
              "FunctionName": "$function"
            },
            "statistics": ["Sum"],
            "period": "300"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 }
      },
      {
        "id": 9,
        "title": "Lambda Errors Over Time",
        "type": "timeseries",
        "targets": [
          {
            "datasource": "CloudWatch-UserMetrics",
            "namespace": "AWS/Lambda",
            "metricName": "Errors",
            "dimensions": {
              "FunctionName": "$function"
            },
            "statistics": ["Sum"],
            "period": "300"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 }
      }
    ]
  },
  "overwrite": true
}
