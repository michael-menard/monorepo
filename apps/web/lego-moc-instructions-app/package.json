{
  "name": "@repo/lego-moc-instructions-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "./scripts/dev.sh",
    "start": "./scripts/dev.sh",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "NODE_OPTIONS='--max-old-space-size=4096' vitest run",
    "test:run": "NODE_OPTIONS='--max-old-space-size=8192' vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:auth": "./scripts/run-auth-tests.sh",
    "test:auth:headed": "./scripts/run-auth-tests.sh --headed",
    "test:auth:debug": "./scripts/run-auth-tests.sh --debug --headed",
    "test:auth:setup": "./scripts/run-auth-tests.sh --setup-only",
    "test:auth:signin": "./scripts/run-auth-tests.sh --test=complete-auth-flow.spec.ts",
    "test:auth:profile": "./scripts/run-auth-tests.sh --test=profile-routing.spec.ts",
    "test:auth:password-reset": "./scripts/run-auth-tests.sh --test=password-reset-flow.spec.ts",
    "test:auth:session": "./scripts/run-auth-tests.sh --test=session-management.spec.ts",
    "test:auth:security": "./scripts/run-auth-tests.sh --test=account-security.spec.ts",
    "test:auth:rbac": "./scripts/run-auth-tests.sh --test=role-based-access-control.spec.ts",
    "test:auth:email": "./scripts/run-auth-tests.sh --test=email-verification-flow.spec.ts",
    "test:auth:manual": "./scripts/run-auth-tests.sh --test=manual-email-verification.spec.ts",
    "test:auth:security-edge": "./scripts/run-auth-tests.sh --test=security-edge-cases.spec.ts",
    "test:auth:performance": "./scripts/run-auth-tests.sh --test=performance-scalability.spec.ts",
    "test:auth:accessibility": "./scripts/run-auth-tests.sh --test=accessibility-mobile.spec.ts",
    "test:auth:privacy": "./scripts/run-auth-tests.sh --test=data-privacy-compliance.spec.ts",
    "test:auth:all": "./scripts/run-auth-tests.sh",
    "test:forgot-password": "playwright test tests/auth/forgot-password.spec.ts",
    "test:forgot-password-e2e": "playwright test tests/auth/forgot-password-e2e.spec.ts",
    "test:ethereal-setup": "tsx src/pages/auth/EmailVerificationPage/__tests__/setup-ethereal.ts",
    "test:mailhog-setup": "tsx src/pages/auth/EmailVerificationPage/__tests__/setup-mailhog.ts",
    "test:ethereal-config": "tsx test-ethereal-email.ts",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "check": "prettier --write src/ && eslint --fix src/",
    "type-check": "tsc --noEmit",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.4.0",
