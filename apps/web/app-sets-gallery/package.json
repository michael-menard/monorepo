{
  "name": "@repo/app-sets-gallery",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "description": "App Sets Gallery application",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "build:check": "vite build && tsc",
    "serve": "vite preview",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ --fix",
    "format": "prettier --write src/",
    "check": "prettier --write src/ && eslint --fix src/",
    "type-check": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "msw:init": "msw init public/ --save",
    "test:msw": "vitest run --config vitest.config.msw.ts"
  },
  "dependencies": {
    "@repo/accessibility": "workspace:*",
    "@repo/app-component-library": "workspace:*",
    "@repo/api-client": "workspace:*",
    "@repo/cache": "workspace:*",
    "@repo/design-system": "workspace:*",
    "@repo/gallery": "workspace:*",
    "@repo/logger": "workspace:*",
    "@repo/mock-data": "workspace:*",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-table": "^8.20.1",
    "@tanstack/react-router-devtools": "^1.130.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "12.23.24",
    "lucide-react": "^0.476.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.5",
    "eslint": "^9.30.1",
    "jsdom": "^26.0.0",
    "msw": "^2.6.4",
    "prettier": "^3.5.3",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vitest": "^3.0.5"
  }
}
