{
  "compilerOptions": {
    "baseUrl": ".",
    "composite": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "noImplicitAny": false,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "tsBuildInfoFile": "./node_modules/.cache/tsbuildinfo-lego-projects-ui.json",
    "paths": {
      "@/store": ["./src/store/index.ts"],
      "@/store/*": ["./src/store/*"],
      "@/components": ["./src/components"],
      "@/components/*": ["./src/components/*"],
      "@/services": ["./src/services"],
      "@/services/*": ["./src/services/*"],
      "@/types": ["./src/types"],
      "@/types/*": ["./src/types/*"],
      "@repo/auth": ["../../../packages/auth/src/index.ts"],
      "@repo/auth/*": ["../../../packages/auth/src/*"],
      "@repo/ui": ["../../../packages/ui/src/index.ts"],
      "@repo/ui/*": ["../../../packages/ui/src/*"]
    }
  },
  "include": [
    "src",
    "src/pages/Wishlist/WishlistSchemas",
    "../../packages/auth/src",
    "../../packages/ui/src",
    "../../features/profile/src/**/*"
  ],
  "files": [],
  "references": [
    { "path": "../../../packages/auth" },
    { "path": "../../../packages/ui" }
  ]
}
