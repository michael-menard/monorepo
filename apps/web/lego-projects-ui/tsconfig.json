      "@repo/profile": ["../../../features/profile/src/index.ts"],
      "@repo/profile/*": ["../../../features/profile/src/*"],
      "@repo/moc": ["../../../features/moc/src/index.ts"],
      "@repo/moc/*": ["../../../features/moc/src/*"]
