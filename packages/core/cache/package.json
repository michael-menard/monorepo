{
  "name": "@repo/cache",
  "version": "1.0.0",
  "description": "Shared caching utilities for the monorepo",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/schemas/cache.ts",
      "types": "./src/schemas/cache.ts"
    },
    "./utils/memoryCache": {
      "import": "./src/utils/memoryCache.ts",
      "types": "./src/utils/memoryCache.ts"
    },
    "./utils/storageCache": {
      "import": "./src/utils/storageCache.ts",
      "types": "./src/utils/storageCache.ts"
    },
    "./utils/imageCache": {
      "import": "./src/utils/imageCache.ts",
      "types": "./src/utils/imageCache.ts"
    },
    "./utils/rtkQueryCache": {
      "import": "./src/utils/rtkQueryCache.ts",
      "types": "./src/utils/rtkQueryCache.ts"
    },
    "./utils/serverlessCache": {
      "import": "./src/utils/serverlessCache.ts",
      "types": "./src/utils/serverlessCache.ts"
    },
    "./managers/IntelligentCacheManager": {
      "import": "./src/managers/IntelligentCacheManager.ts",
      "types": "./src/managers/IntelligentCacheManager.ts"
    },
    "./rtk/IntelligentRTKCacheEnhancer": {
      "import": "./src/rtk/IntelligentRTKCacheEnhancer.ts",
      "types": "./src/rtk/IntelligentRTKCacheEnhancer.ts"
    },
    "./analytics/CacheAnalytics": {
      "import": "./src/analytics/CacheAnalytics.ts",
      "types": "./src/analytics/CacheAnalytics.ts"
    },
    "./utils/performanceMonitor": {
      "import": "./src/utils/performanceMonitor.ts",
      "types": "./src/utils/performanceMonitor.ts"
    },
    "./utils/serverlessCacheManager": {
      "import": "./src/utils/serverlessCacheManager.ts",
      "types": "./src/utils/serverlessCacheManager.ts"
    },
    "./hooks/useServerlessCache": {
      "import": "./src/hooks/useServerlessCache.ts",
      "types": "./src/hooks/useServerlessCache.ts"
    },
    "./hooks/useIntelligentCache": {
      "import": "./src/hooks/useIntelligentCache.ts",
      "types": "./src/hooks/useIntelligentCache.ts"
    },
    "./schemas/cache": {
      "import": "./src/schemas/cache.ts",
      "types": "./src/schemas/cache.ts"
    }
  },
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --fix src/",
    "format": "prettier --write src/",
    "check": "prettier --write src/ && eslint --fix src/",
    "type-check": "tsc --noEmit",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@repo/logger": "workspace:*",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.19.1",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^9.30.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "5.8.3",
    "vite": "^5.2.10",
    "vitest": "^3.0.5"
  }
}
