{
  "name": "@repo/cache",
  "version": "1.0.0",
  "description": "Shared caching utilities for the monorepo",
  "type": "module",
  "main": "./dist/shared-cache.js",
  "browser": "./dist/shared-cache.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "browser": "./dist/shared-cache.js",
      "import": "./dist/shared-cache.js",
      "require": "./dist/shared-cache.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "check": "prettier --write src/ && eslint --fix src/",
    "type-check": "tsc --noEmit",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.25.76",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "framer-motion": "^12.23.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4",
    "lucide-react": "^0.468.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^9.30.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "typescript": "5.8.3",
    "vite": "^5.2.10",
    "vitest": "^3.0.5",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0"
  }
}
