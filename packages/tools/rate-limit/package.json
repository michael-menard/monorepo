{
  "name": "@repo/rate-limit",
  "version": "1.0.0",
  "description": "Framework-agnostic rate limiting with store abstraction",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "check-types": "tsc --noEmit",
    "lint": "eslint --fix src/ --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "typescript": "^5.8.0",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "rate-limit",
    "throttle",
    "api",
    "middleware"
  ]
}
