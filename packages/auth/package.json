{
  "name": "@repo/auth",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --project tsconfig.json",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --reporter=verbose TanStackRouteGuard.unit.test.ts",
    "test:ux": "vitest run --reporter=verbose TanStackRouteGuard.ux.test.tsx",
    "test:performance": "vitest run --reporter=verbose TanStackRouteGuard.performance.test.ts",
    "test:security": "vitest run --reporter=verbose TanStackRouteGuard.security.test.ts",
    "test:authState": "vitest run --reporter=verbose authState.unit.test.ts",
    "test:coverage": "vitest run --coverage",
    "test:all": "vitest run --reporter=verbose"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@reduxjs/toolkit": "^2.0.1",
    "@repo/ui": "workspace:*",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}