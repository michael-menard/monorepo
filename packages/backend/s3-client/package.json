{
  "name": "@repo/s3-client",
  "version": "1.0.0",
  "description": "Shared S3 client utilities for Lambda functions",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "typescript": "^5.8.0"
  },
  "keywords": [
    "s3",
    "aws",
    "lambda",
    "storage",
    "upload"
  ]
}
