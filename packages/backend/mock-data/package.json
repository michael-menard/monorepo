{
  "name": "@repo/mock-data",
  "version": "0.1.0",
  "description": "Centralized mock data for LEGO MOC Instructions App",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint --fix src/",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.0.4"
  },
  "devDependencies": {
    "@types/node": "24.9.1",
    "eslint": "^9.30.1",
    "tsup": "^8.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./dist/index.js",
      "types": "./src/index.ts"
    },
    "./wishlist": {
      "import": "./src/wishlist.ts",
      "require": "./dist/wishlist.js",
      "types": "./src/wishlist.ts"
    },
    "./moc-instructions": {
      "import": "./src/moc-instructions.ts",
      "require": "./dist/moc-instructions.js",
      "types": "./src/moc-instructions.ts"
    },
    "./profile": {
      "import": "./src/profile.ts",
      "require": "./dist/profile.js",
      "types": "./src/profile.ts"
    },
    "./store": {
      "import": "./src/store.ts",
      "require": "./dist/store.js",
      "types": "./src/store.ts"
    }
  }
}
