{
  "name": "@repo/shared-cache",
  "version": "1.0.0",
  "description": "Shared caching utilities for the monorepo",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "check": "prettier --write src/ && eslint --fix src/",
    "type-check": "tsc --noEmit",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^9.30.1",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.7.2",
    "vite": "^5.2.10",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
