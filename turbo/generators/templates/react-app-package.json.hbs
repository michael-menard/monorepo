{
  "name": "@repo/{{kebabCase name}}",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "description": "{{description}}",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "build:check": "vite build && tsc",
    "serve": "vite preview",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ --fix",
    "format": "prettier --write src/",
    "check": "prettier --write src/ && eslint --fix src/",
    "type-check": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "msw:init": "msw init public/ --save",
    "test:msw": "vitest run --config vitest.config.msw.ts"{{#if includeStorybook}},
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"{{/if}}
  },
  "dependencies": {
    {{#if includeAuth}}"@aws-amplify/ui-react": "6.13.1",
    "aws-amplify": "6.15.7",{{/if}}
    {{#if includeRedux}}"@reduxjs/toolkit": "^2.8.2",
    "react-redux": "^9.2.0",{{/if}}
    "@repo/accessibility": "workspace:*",
    "@repo/api-client": "workspace:*",
    "@repo/cache": "workspace:*",
    "@repo/design-system": "workspace:*",
    "@repo/logger": "workspace:*",
    "@repo/ui": "workspace:*",
    {{#if (eq type "module")}}"@repo/mock-data": "workspace:*",{{/if}}
    {{#if (eq type "shell")}}"@repo/gallery": "workspace:*",
    "@repo/moc-instructions": "workspace:*",
    "@repo/dashboard": "workspace:*",
    "@repo/wishlist": "workspace:*",{{/if}}
    {{#if includeRouting}}"@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.130.2",{{/if}}
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "12.23.24",
    "lucide-react": "^0.476.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.5",
    "eslint": "^9.30.1",
    "jsdom": "^26.0.0",
    "msw": "^2.6.4",
    "prettier": "^3.5.3",
    {{#if includeStorybook}}"@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "storybook": "^8.4.7",{{/if}}
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vitest": "^3.0.5"
  }
}
